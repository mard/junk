{
    "manifestVersion": 1,
    "id": "codenames",
    "publisher": "mard",
    "version": "0.0.6",
    "name": "Codenames",
    "description": "Generates unique codenames for work items.",
    "public": false,
    "categories": [
        "Plan and track"
    ],
    "tags": [
        "Work Item",
        "Work Item control"
    ],
    "targets": [
        {
            "id": "Microsoft.VisualStudio.Services"
        }
    ],
    "icons": {
        "default": "logo.png"
    },
    "branding": {
        "color": "rgb(127, 0, 255)",
        "theme": "light"
    },
    "content": {
        "details": {
            "path": "overview.md"
        }
    },
    "files": [
        {
            "path": "static",
            "addressable": true
        },
        {
            "path": "dist",
            "addressable": true
        }
    ]
}
